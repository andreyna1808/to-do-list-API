  "paths": {
    "/users": {
      "post": {
        "tags": ["Users"],
        "summary": "Create a new user",
        "description": "Create a new user",
        "requestURL": "http://localhost:3333/api/v1/users",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "Andreyna",
                  "email": "andreyna@gmail.com",
                  "password": "12345678"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "409": {
            "description": "There is already one user with this email"
          }
        }
      },
      "get": {
        "tags": ["Users"],
        "summary": "View all Users",
        "description": "View all Users, necessary -> Authorization: Bearer <token>",
        "responses": {
          "200": {
            "description": "success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": "string",
                      "email": "string",
                      "created_at": "Date"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
